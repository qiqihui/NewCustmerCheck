@model NewCustomerCheck.Controllers.TaoBaoController.TaoBaoSumModel
@{
    ViewData["Title"] = Model.ActivityName + "淘宝线下总数据查询";
}
<!-- page content -->
<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>淘宝接口 —— @Model.ActivityName</h3>
            </div>

            <div class="title_right">

            </div>
        </div>
        <div class="clearfix"></div>
        @if (!Model.NoData)
        {
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>淘宝线下总数据 <small>@Model.ActivityName</small></h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                                    <ul class="dropdown-menu" role="menu">
                                        <li>
                                            <a href="#">Settings 1</a>
                                        </li>
                                        <li>
                                            <a href="#">Settings 2</a>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <a class="close-link"><i class="fa fa-close"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <p class="text-muted font-13 m-b-30">
                                点击下面三个按钮可生成文件下载
                            </p>
                            <table id="datatable-buttons" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>活动ID</th>
                                        <th>首购用户数</th>
                                        <th>结算CPA 奖励金额</th>
                                        <th>当日激活且首购结算的CPA 金额</th>
                                        <th>当日激活且首购的有效用户数</th>
                                        <th>当日绑卡有效用户数</th>
                                        <th>日期</th>
                                        <th>新激活用户数</th>
                                        <th>确认收货用户数</th>
                                        <th>结算有效用户数</th>
                                        <th>购买有效用户数</th>
                                        <th>新注册用户数</th>
                                        <th>渠道关系id</th>
                                        <th>有效数字</th>
                                    </tr>
                                </thead>


                                <tbody>
                                    @foreach (var item in Model.DataModels)
                                    {
                                        <tr>
                                            <td>@item.ActivityId</td>
                                            <td>@item.AlipayUserCnt</td>
                                            <td>@item.AlipayUserCpaPreAmt</td>
                                            <td>@item.BindBuyUserCpaPreAmt</td>
                                            <td>@item.BindBuyValidUserCnt</td>
                                            <td>@item.BindCardValidUserCnt</td>
                                            <td>@item.BizDate</td>
                                            <td>@item.LoginUserCnt</td>
                                            <td>@item.RcvUserCnt</td>
                                            <td>@item.RcvValidUserCnt</td>
                                            <td>@item.ReBuyValidUserCnt</td>
                                            <td>@item.RegUserCnt</td>
                                            <td>@item.RelationId</td>
                                            <td>@item.ValidNum</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        }
        else
        {
            <div class="alert alert-danger alert-dismissible fade in" role="alert">
                <button class="close" aria-label="Close" type="button" data-dismiss="alert">
                    <span aria-hidden="true">×</span>
                </button>
                <strong>没有数据！</strong> 请选择其他活动
            </div>
        }

    </div>
</div>
<!-- /page content -->
